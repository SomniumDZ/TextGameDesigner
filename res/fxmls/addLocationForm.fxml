<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox alignment="TOP_RIGHT" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="UIControllers.AddLocationFormController">
  <children>
    <GridPane alignment="CENTER_RIGHT" hgap="5.0" vgap="10.0">
      <children>
        <Label text="Name" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <TextField fx:id="nameField" prefWidth="50.0" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="0" />
        <Label text="Background Image" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <HBox prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
          <children>
            <TextField fx:id="imageField" prefWidth="200.0" HBox.hgrow="SOMETIMES" />
            <Button fx:id="browseImageButton" mnemonicParsing="false" text="..." />
          </children>
        </HBox>
      </children>
      <columnConstraints>
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="-1.0" prefWidth="-1.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="-1.0" prefHeight="-1.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="-1.0" prefHeight="-1.0" valignment="CENTER" vgrow="SOMETIMES" />
      </rowConstraints>
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
      </VBox.margin>
    </GridPane>
    <ImageView fx:id="previewImageView" fitHeight="192.0" fitWidth="341.0" pickOnBounds="true" preserveRatio="true" VBox.margin="$x1" />
    <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
      <children>
        <Button fx:id="okButton" mnemonicParsing="false" text="Ok" />
        <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
      </children>
      <padding>
        <Insets top="20.0" />
      </padding>
    </HBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
